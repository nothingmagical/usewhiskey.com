<% @versions.each_with_index do |item, i| %>
<% item_version = item.css('enclosure').first['sparkle:version'].to_i %>
<% next if @version > 0 && item_version > @version %>
<h3><%= item.css('title').inner_text %></h3>
<p><em>Released <%= Date.parse(item.at_xpath('.//pubDate')) %></em></p>
<%= item.at_xpath('.//description').inner_text %>
<% if !@is_sparkle && i == 0 %>
<p><a href="/latest">Download</a></p>
<% end %>
<hr>
<% end %>
